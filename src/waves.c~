#include <SFML/Audio.h>
#include <stdio.h>
char *read_fonction(char *file)
{
    int size = 10000;
    char *buffer = malloc(sizeof(char) * 1000);
    read(open(file, O_RDONLY), buffer, size);
    return buffer;
}

int main(void)
{
    char *waves = read_fonction("waves.txt");
    //my_putstr(waves);
    printf("%s, waves");
}
